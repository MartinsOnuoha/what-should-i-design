{
  "name": "what-should-i-design",
  "version": "0.0.0",
  "license": "(MIT OR Apache-2.0)",
  "keywords": [
    "ui/ux",
    "problem-statment",
    "what-should-i-design",
    "design"
  ],
  "author": {
    "email": "martinsvictor.nd@gmail.com",
    "name": "MartinsOnuoha",
    "url": "https://github.com/MartinsOnuoha"
  },
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "commit": "npm run type-check && npm run lint && npm run format && git add . && cz && git push origin head"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@vue/apollo-composable": "^4.0.0-beta.11",
    "@vueuse/core": "^10.5.0",
    "click-outside-vue3": "^4.0.1",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "lodash.shuffle": "^4.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.5",
    "@types/lodash.shuffle": "^4.2.9",
    "@types/node": "^20.9.0",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-emoji-conventional": "^1.0.2",
    "eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MartinsOnuoha/what-should-i-design"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji-conventional"
    }
  }
}
