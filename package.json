  "license": "(MIT OR Apache-2.0)",
  "keywords": [
    "ui/ux",
    "problem-statment",
    "what-should-i-design",
    "design"
  ],
  "author": {
    "email": "martinsvictor.nd@gmail.com",
    "name": "MartinsOnuoha",
    "url": "https://github.com/MartinsOnuoha"
  },
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "commit": "npm run type-check && npm run lint && npm run format && git add . && cz && git push origin head"
  },
  "dependencies": {
    "@apollo/client": "^3.10.5",
    "@vue/apollo-composable": "^4.0.2",
    "@vueuse/core": "^10.11.0",
    "click-outside-vue3": "^4.0.1",
    "graphql": "^16.8.2",
    "graphql-tag": "^2.12.6",
    "lodash.shuffle": "^4.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.27",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node18": "^18.2.4",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/lodash.shuffle": "^4.2.9",
    "@types/node": "^20.14.7",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-emoji-conventional": "^1.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "sass": "^1.77.5",
    "tailwindcss": "^3.4.4",
    "typescript": "~5.4.5",
    "vite": "^5.3.0",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MartinsOnuoha/what-should-i-design"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji-conventional"
    }
  }
}
